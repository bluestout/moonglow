.s4com-helpcenter{
  .s4com-space-bottom{
    border: 0;
    .s4com-panel{
      padding: 10px;
      padding-bottom: 9px;
      background-color: #F8F7F5;
      border-radius: 0;
      .s4com-clearfix{
        &:after{
          content: " ";
          background: url(//cdn.shopify.com/s/files/1/2486/3224/t/224/assets/search_ico.svg?v=7438112706812041255) no-repeat;
          background-position: center;
          background-size: contain;
          width: 20px;
          height: 20px;
          display: block;
          position: absolute;
          left: auto;
          right: 20px;
          top: 20px;
        }
      }
    }
    .s4com-top-heading{
      display: none;
    }
    .s4com-sub-heading{
      border: 0;
      position: relative;
      padding-left: 10px;
      span{
        position: absolute;
        left: auto;
        right: 10px;
      }
    }
    .s4com-section-body{
      padding-left: 10px;
      .s4com-articles{
        padding-left: 0px;
        border: none;
        a.js-no-transition{
          padding: 0 10px;
          border: 1px solid #E0E6ED;
          display: block;
          margin-bottom: 15px;
          .s4com-icon-wrap{
            right: 10px;
            left: auto;
          }
          .s4com-article-title {
            padding: 10px 20px;
            color: black!important;
            font-family: 'Montserrat Medium'!important;
            font-size: 14px;
          }
        }
        .s4com-article-body{
          &:focus{
            border: none;
            outline: none;
          }
