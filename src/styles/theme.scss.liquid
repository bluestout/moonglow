      justify-content: center;
      max-width: 596px;
      width: 100%;
      position: relative;
      .swell-referral-back-link-holder {
        position: absolute;
          top: 10px;
          right: 25px;
          .swell-icon-close {
            font-size: 16px;
            cursor: pointer;
          }
      }

      .swell-referral-copy-main {
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        padding-top: 70px;
        padding-bottom: 70px;
        .swell-referral-back-link {
          position: absolute;
          top: 10px;
          right: 15px;
          cursor: pointer;
          width: 1em;
          height: 1em;
          max-width: 24px;
          max-height: 24px;
          display: block;
          vertical-align: middle;
          &:after,
          &:before {
            background: #000;
            content: "";
            height: 2px;
            max-width: calc(14px * 1.414);
            position: absolute;
            top: 0;
            transition: 0.05s ease-in-out background-color;
            width: calc(2em * 1.414);
          }
          &:before {
            left: 0;
            -webkit-transform: rotate(45deg);
            -ms-transform: rotate(45deg);
            transform: rotate(45deg);
            -webkit-transform-origin: left;
            -ms-transform-origin: left;
            transform-origin: left;
          }
          &:after {
            right: 0;
            -webkit-transform: rotate(-45deg);
            -ms-transform: rotate(-45deg);
            transform: rotate(-45deg);
            -webkit-transform-origin: right;
            -ms-transform-origin: right;
            transform-origin: right;
          }
        }
        .swell-referral-heading {
          display: none;
        }
        .swell-referral-form-wrapper {
          display: flex;
          align-items: center;
          justify-content: center;
          flex-direction: column;
          position: relative;
          .swell-referral-copy-link {
            font-size: 24px;
            font-weight: 700;
            margin-bottom: 25px;
            color: #000;
          }
          .swell-referral-copy-button {
            background-image: url('swell-copy-link-btn.jpg');
            background-position: center;
            background-size: cover;
            background-color: transparent;
            color: transparent;
            padding: 18px 80px 18px 20px;
            margin-bottom: 35px;
          }

          .swell-icon-right-arrow {
            position: absolute;
            top: 20px;
            color: #ffffff;
            right: 80px;
          }

          .swell-referral-details {
            color: #48484a;
          }
        }
      }
    }
  }

