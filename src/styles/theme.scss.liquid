.search-form{
    position: relative;
    margin-bottom: 0px!important;
    &:after{
        content: " ";
        background: url({{ 'search_ico.svg' | asset_url }}) no-repeat 50% 50% / contain;
        width: 18px;
        height: 41px;
        display: block;
        position: absolute;
        right: 14px;
        top: 0;
    }
}

.snize-filters-sidebar{
  margin-right: 0px!important;
}
.snize-search-results-main-content{
  float: right;
}

.menu{
    .link-list{
        .active{
            &:after{
                content: " ";
                background: url({{ 'arrow_left.svg' | asset_url }}) no-repeat 50% 50% / contain;
                filter: brightness(10);
                width: 17px;
                height: 17px;
                display: table;
                float: right;
                transform: rotatez(-90deg);
            }
        }
    }
}



// swell reward scss


.swell-referral-back-link {
    font-family: 'swell-icomoon-' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
  }



  .break-tag {
    display: none;
  }

  #main_content {
    .swell-moon-glow {
    }
  }

  #main_content.swell-account-rewards {
    padding-bottom: 0px;
  }

  .swell-hide{
    display: none !important;
  }


  #swell-checkout {
    .footer-holder {
      display: flex;
      justify-content: space-between;
      margin-top: 20px;

      .swell-redeem {
        display: flex;
        flex-direction: column;
        .redeeem-text {
          color: #2e4049;
          margin-bottom: 10px;
        }

        .redeem-content-holder {
          color: #2e4049;
        }
      }

      #swell-checkout-redeem-btn
      {
        width: 90px;
        height: 46px;
        float: right;
        padding: 13px 25px;
      }
    }
    .rangeslider--horizontal {
      border-radius: 0;
      background-color: #e4e4e4;
      height: 12px;
      margin-bottom: 20px;
      .rangeslider__handle {
        top: -9px;
        width: 25px;
        height: 25px;
        border-radius: 0;
        background-color: white;
        &:after
        {
          width:0;
          height: 0;
        }
      }
      .rangeslider__fill {
        background: #2E4676 !important;
        border-radius: 0;
      }
    }
    .fieldset
    {
      .field
      {
        .redeem-credits-title {
          font-weight: 400;
          .swell-point-balance {
            color: #2f4876;
          }
        }

        .input-holder {
          margin-top: 20px;
        }

        .footer-holder {
          .redeem-content
          {
            font-size: 15px;
            flex: 1;
            color: #2e4049;
          }
        }
      }
    }
  }


  .swell-moon-glow {
    .swell-referral-section {
      position: relative;
      background-image: url(swell-referral-bg.jpg);
      background-repeat: no-repeat;
      background-size: cover;
      background-position: center;
      width: 100%;
      min-height: 70vh;
      display: flex;
      align-items: center;
      justify-content: flex-start;
      padding: 50px 5%;
      @media (min-width: 768px) {
        min-height: 100vh;
      }
        .swell-referral-section-container {
          display: flex;
          width: 100%;
        }
      .swell-referral {
        background-color: #d0d9e4;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-right: 10px;
        padding-top: 50px;
        padding-bottom: 50px;
        position: relative;
        .swell-referral-content-sidebar {
          display: none;
        }
        .swell-referral-content-container {
          .swell-referral-content-main {
            .heading {
              display: none;
            }

            .swell-referral-register,.swell-referral-refer {
              display: flex;
              align-items: center;
              justify-content: center;
              flex-direction: column;
              padding-left: 5px;
              padding-right: 5px;
              .swell-referral-heading-refer {
                font-size: 18px;
                color: #2f4775;
              }
              .swell-referral-heading {
                color: #2f4775;
                font-size: 36px;
              }
              .swell-referral-details {
                color: #2f4775;
                font-size: 16px;
                max-width: 456px;
                text-align: center;
                margin-bottom: 30px;
                font-weight: 500;
              }
              .swell-referral-form-wrapper {
                max-width: 480px;
                width: 100%;
                margin-bottom: 30px;
                .swell-referral-form-body {
                  .refer-customer-error {
                    color: #ff0000;
                  }
                  .swell-referral-form-list {
                    .swell-referral-form-list-field {
                      display: flex;
                      flex-direction: row;
                      position: relative;
                      .swell-referral-form-list-field-input {
                        font-family: "Montserrat",sans-serif;
                        font-weight: 300;
                        background-color: #eaf0f7;
                        color: #46484a;
                        flex-basis: 85%;
                        padding: 15px 10px;
                        font-size: 14px;
                      }
                      .swell-referral-form-list-submit {
                        background-image: url('swell-moon-submit.jpg');
                        background-position: center;
                        background-size: cover;
                        background-color: transparent;
                        color: transparent;
                        border: 0;
                        flex-basis: 15%;
                        cursor: pointer;
                      }
                      .swell-referral-form-list-submit-icon {
                        cursor:pointer;
                        color: #ffffff;
                        position: absolute;
                        right: 28px;
                        top:16px;
                        transform: rotate(25deg);
                          width: 16px;
                          height: 16px;
                          font-size: 16px;
                          .path1:before {
                            color: #ffffff;
                          }
                      }
                    }
                  }
                }
              }
            }

            .swell-referral-refer {
              .swell-referral-media-wrapper {
                color: #2f4775;
                .swell-referral-media-details {
                  font-size: 14px;
                  margin-bottom: 15px;
                }

                .swell-referral-media-list {
                  display: flex;
                  align-items: center;
                  justify-content: center;
                  max-width: 365px;
                  margin: 0 auto;
                  .swell-referral-medium {
                    cursor: pointer;
                    flex: 1;
                    font-size: 14px;
                    .swell-referral-medium-content {
                      .swell-referral-media-icon {
                        margin-right: 5px;
                      }
                    }
                  }

                  .swell-mobile {
                    display: none;
                  }
                }
              }
            }

            .swell-referral-thanks {
              display: flex;
              flex-direction: column;
              align-items: center;
              justify-content: center;
              min-height: 339px;
              .swell-referral-back-link {
                position: absolute;
                top: 60px;
                left: 0;
                right: 0;
                margin: 0 auto;
                cursor: pointer;
                width: 1em;
                height: 1em;
                max-width: 24px;
                max-height: 24px;
                display: block;
                vertical-align: middle;
                &:after,
                &:before {
                  background: #000;
                  content: "";
                  height: 2px;
                  max-width: calc(14px * 1.414);
                  position: absolute;
                  top: 0;
                  transition: 0.05s ease-in-out background-color;
                  width: calc(2em * 1.414);
                }
                &:before {
                  left: 0;
                  -webkit-transform: rotate(45deg);
                  -ms-transform: rotate(45deg);
                  transform: rotate(45deg);
                  -webkit-transform-origin: left;
                  -ms-transform-origin: left;
                  transform-origin: left;
                }
                &:after {
                  right: 0;
                  -webkit-transform: rotate(-45deg);
                  -ms-transform: rotate(-45deg);
                  transform: rotate(-45deg);
                  -webkit-transform-origin: right;
                  -ms-transform-origin: right;
                  transform-origin: right;
                }
              }
            }
          }
        }
      }

      .redeem-holder {
        background-color: #2f4775;
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        color: #ffffff;
        padding: 50px 10px;

        .check-reward-balance {
          .check-reward-balance-heading {
            font-size: 16px;
            text-decoration: underline;
            color: #ffffff;
          }
        }

        .refer-details {
          .swell-section-content {
            border-bottom: 1px solid #55688e;
            padding-bottom: 10px;
            margin: 0 auto 25px;
          }

          .swell-section-heading {
            text-align: center;
          }

          .table-wrapper {
            max-height: 137px;
            min-height: 137px;
              overflow-y: scroll;
              overflow-x: hidden;
              padding-right: 20px;
            .swell-referral-table {
              border-collapse: collapse;
                border-spacing: 0;
                width: 100%;
                border: 0;
              .swell-table-row {
                border-top: 0;
              }
              .swell-th {
                font-weight: 300;
                text-transform: capitalize;
                border-bottom: 1px solid #55688e;
                text-align: left;
                color: #ffffff;
                padding: 10px 12px;
                font-size: 18px;
              }
              .swell-td {
                display: table-cell;
                float: none;
                border-bottom: 1px solid #55688e;
                text-align: left;
                padding: 10px 12px;
                color: #ffffff;
                font-size: 18px;
                &:first-child {
                  max-width: 299px;
                  width: 299px;
                  overflow: hidden;
                  text-overflow: ellipsis;
                  white-space: nowrap;
                }
              }
            }
            &::-webkit-scrollbar-track {
              border-radius: 10px;
              background-color: #e7e7e7;
            }

            &::-webkit-scrollbar {
              width: 8px;
              border-radius: 10px;
              background-color: #020202;
            }

            &::-webkit-scrollbar-thumb {
              border-radius: 10px;
              background-color: #020202;
            }
          }

        }
        .heading-area {
          max-width: 370px;
          margin: 0 auto;
          .heading-holder {
            text-align: center;
            max-width: 312px;
            margin: 0 auto;
            .heading {
              font-size: 27px;
              text-transform: capitalize;
              line-height: 1.2;
            }
          }
          .heading-text {
            text-align: center;
            font-size: 14px;
          }
        }
      }
    }

    .swell-referral-loader {
      display: flex;
      align-items: center;
      justify-content: center;
      position: absolute;
      top: 30%;
      left: 0;
      right: 0;
      .big-spinner {
        max-width: 75px;
        width: 100%;
      }
    }
  }

  .swell-referral-copy {
