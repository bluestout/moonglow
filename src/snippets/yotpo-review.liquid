<!-- yotpo-review -->
<script defer="defer">
  {%- if settings.app_yotpo_key == blank -%}
  console.error("Please add yotpo key using theme settings!");
  {%- else -%}
  (function e() {
    var e = document.createElement("script");
    e.type = "text/javascript",
    e.async = true,
    e.src = "//staticw2.yotpo.com/{{settings.app_yotpo_key}}/widget.js";
    var t = document.getElementsByTagName("script")[0];
    t.parentNode.insertBefore(e, t)
  })();
  {%- endif -%}
</script>