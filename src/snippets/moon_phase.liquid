<div id="moon-phase" class="modal" data-target="">
    {% if page.handle == 'wholesale' %}
      <a class="close"> back <img src="{{'close_ico.png' | asset_url}}" alt="close button"></a>
    {% elsif page.handle == 'find-your-moon'  %}
      <a class="close">{{ 'general.modal.close_button_2' | t }} <img src="{{'close_ico.png' | asset_url}}" alt="close button"></a>
    {% else %}
      <a class="close"> {{ 'general.modal.close_button_1' | t }} <img src="{{'close_ico.png' | asset_url}}" alt="close button"></a>
    {% endif %}

    <span class="back" page-target="" ><img src="{{'arrow_left.svg' | asset_url}}" alt="arrow left"> {{ 'general.modal.back_button' | t }} </span>
