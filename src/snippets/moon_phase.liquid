          <div class="select box">
            <button class="button grey start-btn">{{ section.settings.moon_phase_start_button }}</button>
          </div>
      </div>
      <div class="field calendars">
        <div class="calendar">
          <div class="date-header">
            <svg width="11px" height="13px" viewBox="0 0 11 13" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
              <!-- Generator: sketchtool 51.3 (57544) - http://www.bohemiancoding.com/sketch -->
              <title>moon icon</title>
              <desc>Created with sketchtool.</desc>
              <defs></defs>
              <g id="style-guide" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                  <g id="icons" transform="translate(-170.000000, -34.000000)" fill="#2E4676">
                      <path d="M176.830961,34.0168995 C175.75447,35.1809617 175.09456,36.7507096 175.09456,38.4777778 C175.09456,41.9087874 177.698972,44.7189146 181,44.9608783 C179.838918,46.2164137 178.193212,47 176.3682,47 C172.85114,47 170,44.0898509 170,40.5 C170,36.9101491 172.85114,34 176.3682,34 C176.523821,34 176.678137,34.0056976 176.830961,34.0168995 Z" id="moon_ico"></path>
                  </g>
              </g>
            </svg>
            <p><a class="date">{{ 'general.moon_phase_modal.date_header_title' | t }}</a> / <a class="month">{{ 'general.moon_phase_modal.month_header_title' | t }}</a> / <a class="year">{{ 'general.moon_phase_modal.year_header_title' | t }}</a></p>
          </div>

          <div class="content">
            <div class="content-field year-field">
              <p class="mark uppercase">{{ 'general.moon_phase_modal.year_page_title' | t }}</p>
              <ul>
                  {% for year in (1919..2030) %}
                    <li>{{year}}</li>
                  {% endfor %}
              </ul>
            </div>
            <div class="content-field month-field">
              <p class="title uppercase">{{ 'general.moon_phase_modal.month_page_title' | t }}</p>
              <ul>
                <li>January</li><li>February</li><li>March</li><li>April</li><li>May</li><li>June</li><li>July</li><li>August</li><li>September</li><li>October</li>
                <li>November</li><li>December</li>
              </ul>
            </div>
            <div class="content-field date-field">
              <p class="title uppercase">{{ 'general.moon_phase_modal.date_page_title' | t }}</p>
              <ul>
                  {% for date in (1..31) %}
                    <li>{{ date }}</li>
                  {% endfor %}
              </ul>
            </div>
            <div class="content-field  save-field">

              <div class="grid">
                  <div class="grid__item one-third small--one-whole content-img">
                    <img src="" class="modal_moon_phase_img" alt="PL">
                    <div class="moon-icon">
                        <div></div>
                    </div>

                  </div>
                  <div class="grid_item two-thirds small--one-whole content-info">
                  <p class="mark"></p>
                  <p></p>
                  </div>
              </div>
                       
              {% if page.handle != 'wholesale' %}
                <div class="grid content-pannel" {% if page.handle == 'find-your-moon' %}style="width: 100%; margin-top: 0px;"{% endif %}>
                  {% if page.handle == 'find-your-moon' %}
                    <h3 class="capitalize brand text-center">Your moon phase code : <strong class="uppercase brand moon-code"></strong></h3>
                  {% else %}
                    <a class="button small select-phase" date-val="">{{ section.settings.select_button }}</a>
                    <p class="mark text-center save-date">{{ section.settings.save_button }}</p>
                    <a href="" class="button small shop-date">{{ section.settings.shop_button }}</a>
                  {% endif %}
                </div>
              {% endif %}

            </div>
            <div class="alert-message animated alert-danger bounceIn bounceOut modal_alert" data-text="{{ 'general.alert_message.danger' | t }}" data-logintext="Please login to save the date">
            {{ 'general.alert_message.danger' | t }}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
